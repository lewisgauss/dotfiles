Host *
    IgnoreUnknown AddKeysToAgent,UseKeychain
    AddKeysToAgent yes
    UseKeychain yes

Host github.com
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_ed25519

Host homelab1
    User homelab1
    Hostname 10.0.20.50
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_ed25519_lab

